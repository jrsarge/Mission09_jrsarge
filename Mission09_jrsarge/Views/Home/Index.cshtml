@{
    ViewBag.Calc = 20 / 10;
}

@model BooksViewModel

<h1>@ViewBag.Calc</h1>

<h1>Welcome to Fox Books</h1>

@foreach(Book b in Model.Books)
{
    <h3>@b.Title</h3>

    <ul>
        <li>Author: @b.Author</li>
        <li>Publisher: @b.Publisher</li>
        <li>ISBN: @b.Isbn</li>
        <li>Classification: @b.Classification</li>
        <li>Page Count: @b.PageCount pages</li>
        <li>Price: $ @b.Price</li>
    </ul>

    <br/>
}

<div page-model="@Model.PageInfo"></div>

